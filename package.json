{
  "author": "",
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "axios": "^1.5.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "sequelize": "^6.33.0",
    "winston": "^3.11.0"
  },
  "description": "",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "keywords": [],
  "license": "ISC",
  "main": "index.ts",
  "name": "expenses-graphql",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --watch --config codegen.ts",
    "copy-assets": "cp -r ./migrations ./seeders ./config ./.sequelizerc ./dist/",
    "compile": "tsc",
    "delete:dist": "rm -rf ./dist",
    "build": "pnpm delete:dist && pnpm codegen && pnpm compile && pnpm copy-assets",
    "deploy:prod": "pnpm build && fly deploy",
    "dev": "NODE_ENV=dev nodemon --esm src/index.ts",
    "lint": "eslint . --ext .ts",
    "migrate": "npx sequelize-cli db:migrate",
    "seeds": "npx sequelize-cli db:seed:all",
    "test": "vitest",
    "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"pnpm codegen:watch\"  \"tsc -w \" \"pnpm dev \""
  },
  "graphql": {
    "schema": "http://localhost:4000/graphql",
    "documents": "**/*.{graphql,js,ts}"
  },
  "type": "module",
  "version": "1.0.0"
}
